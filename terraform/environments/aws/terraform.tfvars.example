# AWS Production Configuration Example
# 
# Copy this file to terraform.tfvars and fill in your values:
#   cp terraform.tfvars.example terraform.tfvars
#
# WARNING: terraform.tfvars is gitignored and contains sensitive data

project_name    = "zixly"
environment     = "prod"
aws_region      = "ap-southeast-2" # Sydney

# Trading API Configuration
trading_api_key = "your-production-api-key"
trading_api_url = "https://api.trading.example.com"

# SMTP Configuration (use AWS SES in production)
smtp_host     = "email-smtp.ap-southeast-2.amazonaws.com"
smtp_port     = 587
smtp_user     = "your-ses-smtp-username"
smtp_password = "your-ses-smtp-password"
smtp_from     = "notifications@zixly.com"

